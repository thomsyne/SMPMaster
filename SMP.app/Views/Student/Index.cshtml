@model IEnumerable<SMP.app.Models.Student>
@{
    ViewBag.Title = "Student";
}
<link href="~/temp/vendors/datatables.net-bs/css/dataTables.bootstrap.css" rel="stylesheet" />
<link href="~/temp/vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="~/temp/vendors/datatables.net-buttons-bs/css/buttons.bootstrap.min.css" rel="stylesheet">
<link href="~/temp/vendors/datatables.net-fixedheader-bs/css/fixedHeader.bootstrap.min.css" rel="stylesheet">
<link href="~/temp/vendors/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet">
<link href="~/temp/vendors/datatables.net-scroller-bs/css/scroller.bootstrap.min.css" rel="stylesheet">

    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <a class="btn-sm btn-primary" id="" href="@Url.Action("AddOrEdit","Student")"><i class="fa fa-plus"></i>Add New</a>
            </div>
            <div class="x_content">
                <div id="datatable-buttons_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">
                    <table id="datatable-buttons" class="table table-striped table-bordered dataTable no-footer dtr-inline" role="grid" aria-describedby="datatable-buttons_info">
                        <thead>
                            <tr role="row">
                                
                                <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending">
                                    @Html.DisplayNameFor(model => model.Student_Code)
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending">
                                    @Html.DisplayNameFor(model => model.LastName)
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending">
                                    @Html.DisplayNameFor(model => model.FirstName)
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending">
                                    @Html.DisplayNameFor(model => model.ClassId)
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending">
                                    @Html.DisplayNameFor(model => model.ArmId)
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending">
                                    @Html.DisplayNameFor(model => model.PrefectId)
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending">
                                    @Html.DisplayNameFor(model => model.GenderId)
                                </th>
                                <th class="sorting" tabindex="0" aria-controls="datatable-buttons" rowspan="1" colspan="1" aria-label="Office: activate to sort column ascending">
                                    @Html.DisplayNameFor(model => model.ProgramId)
                                </th>
                                <th style="width:20px;">
                                    (Action)
                                </th>
                            </tr>
                        </thead>
                        @if (Model != null)
                        {
                            foreach (var item in Model)
                            {
                                <tbody>
                                    <tr role="row" class="odd">
                                        

                                        <td tabindex="0" class="sorting_1">
                                            @Html.DisplayFor(modelItem => item.Student_Code)
                                        </td>
                                        <td tabindex="0" class="sorting_1">
                                            @Html.DisplayFor(modelItem => item.LastName)
                                        </td>
                                        <td tabindex="0" class="sorting_1">
                                            @Html.DisplayFor(modelItem => item.FirstName)
                                        </td>
                                        <td tabindex="0" class="sorting_1">
                                            @Html.DisplayFor(modelItem => item.ClassId)
                                        </td>
                                        <td tabindex="0" class="sorting_1">
                                            @Html.DisplayFor(modelItem => item.ArmId)
                                        </td>
                                        <td tabindex="0" class="sorting_1">
                                            @Html.DisplayFor(modelItem => item.PrefectId)
                                        </td>
                                        <td tabindex="0" class="sorting_1">
                                            @Html.DisplayFor(modelItem => item.GenderId)
                                        </td>
                                        <td tabindex="0" class="sorting_1">
                                            @Html.DisplayFor(modelItem => item.ProgramId)
                                        </td>
                                        <td>
                                            <a href="@Url.Action("AddOrEdit","Student", new { id = item.ItbId})" class="btn-sm btn-primary" onclick="">
                                                <span class="fa fa-edit"></span>
                                            </a>
                                        </td>
                                    </tr>
                                </tbody>
                            }
                        }
                    </table>
                </div>
            </div>
        </div>
    </div>

@section Scripts{
    <script src="~/temp/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/temp/vendors/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/temp/vendors/datatables.net-buttons-bs/js/buttons.bootstrap.min.js"></script>
    <script src="~/temp/vendors/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/temp/vendors/datatables.net-buttons/js/buttons.print.min.js"></script>
}

